<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.softartdev.android.dagger2simple">

    <application
        android:name="com.softartdev.android.dagger2simple.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.softartdev.android.dagger2simple.ui.main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.softartdev.android.dagger2simple.ui.second.SecondActivity">
        </activity>
        <activity android:name="com.softartdev.android.dagger2simple.ui.third.ThirdActivity">
        </activity>
    </application>

</manifest>